{% extends "./admin/index.html.twig" %}
{% block intro %}
<div class="container">
    <div class="jumbotron text-center">
        <h1>Holding CAFAO - Administration </h1>
        <h2>Ajouter un administrateur</h2>
    </div>
    {{form_start(form)}}
    <div class="row">
        <div class="col-sm-12 col-sm-3">{{form_row(form.prenom)}}</div> 
        <div class="col-sm-12 col-sm-3">{{form_row(form.nom)}}</div> 
        <div class="col-sm-12 col-sm-3">{{form_row(form.email)}}</div>
        <div class="col-sm-12 col-sm-3">{{form_row(form.password)}}</div> 
    </div>

    <div class="row">
        <div class="col-sm-12 col-sm-4">{{form_row(form.confirmPassword)}}</div> 
    </div>

    <div class="col-sm-12 mb-50 mt-20" style="z-index: 1000;">{{form_row(form.submit)}}</div>

    {{form_end(form)}}
    
        
</div>
{% endblock %}

{% block javascripts %}
  {{ parent() }}
  {# {{ encore_entry_script_tags('new_immeuble') }} #}

{% endblock %}